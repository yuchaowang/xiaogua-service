<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiaogua.comments.dal.mapper.CheckCodeStateMapper">
    <select id="selectMaxByCodeAndCacheKey" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from t_checkcode_state
        where code = #{code}
        and cacheKey = #{cacheKey}
        order by expir desc
        limit 1
    </select>
</mapper>