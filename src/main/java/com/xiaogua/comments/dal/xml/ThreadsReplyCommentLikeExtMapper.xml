<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiaogua.comments.dal.mapper.ThreadsReplyCommentLikeMapper">
  <select id="selectByCommentCodeAndUserCode" parameterType="Map" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from t_threads_reply_comment_like
    where comments_code = #{commentsCode}
      and user_code = #{userCode}
  </select>
</mapper>