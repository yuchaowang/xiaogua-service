<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiaogua.comments.dal.mapper.CommentsReplyLikeMapper">
    <select id="selectByCommentsReplyCodeAndUserCode" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from t_comments_reply_like
        where comments_code = #{commentsReplyCode}
        and user_code = #{userCode}
    </select>
</mapper>